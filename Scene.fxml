<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.web.WebView?>

<Scene xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="applicationController">
    <AnchorPane prefHeight="512.0" prefWidth="721.0">
      <children>
         <WebView id="webView" fx:id="webView" layoutX="-1.0" layoutY="95.0" prefHeight="416.0" prefWidth="722.0" />
         <Button layoutX="-1.0" layoutY="71.0" mnemonicParsing="false" onAction="#loadPage" prefHeight="20.0" prefWidth="46.0" text="Load" />
         <TextField fx:id="textField" layoutY="45.0" onAction="#loadPage" prefHeight="26.0" prefWidth="721.0" />
         <Button layoutX="46.0" layoutY="71.0" mnemonicParsing="false" onAction="#refreshPage" text="Refresh ↺" />
         <Button layoutX="113.0" layoutY="71.0" mnemonicParsing="false" onAction="#zoomIn" text="+" />
         <Button layoutX="138.0" layoutY="71.0" mnemonicParsing="false" onAction="#zoomOut" text="-" />
         <Button layoutX="159.0" layoutY="71.0" mnemonicParsing="false" onAction="#displayHistory" text="History" />
         <Button layoutX="213.0" layoutY="71.0" mnemonicParsing="false" onAction="#back" text="←Back" />
         <Button layoutX="264.0" layoutY="71.0" mnemonicParsing="false" onAction="#forward" text="Forward→" />
      </children></AnchorPane>
</Scene>
